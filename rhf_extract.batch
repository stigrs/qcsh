#!/bin/sh

# Script for extracting RHF information from Gaussian output files.

files=`ls *.log`

for f in $files
do
        en=`grep E\(RHF\) $f | tail -1 | awk '{print $5}'`
        zpe=`grep "Zero-point correction" $f | awk '{print $3}'`
        echo `basename $f .log` : E = $en , ZPE = $zpe
done

